{
  "name": "OctoRobot",
  "version": "0.0.1",
  "description": "Small application for making predictions online",
  "author": "Alexey Raspopov",
  "repository": "alexeyraspopov/octorobot",
  "license": "MIT",
  "dependencies": {
    "express": "~4.13.4",
    "normalize.css": "~3.0.3",
    "react": "~0.14.7",
    "react-dom": "~0.14.7",
    "react-router": "~2.0.0"
  },
  "devDependencies": {
    "autoprefixer": "~6.3.3",
    "babel": "~6.5.2",
    "babel-cli": "~6.6.0",
    "babel-core": "~6.6.0",
    "babel-eslint": "~5.0.0",
    "babel-loader": "~6.2.4",
    "babel-polyfill": "~6.6.1",
    "babel-preset-es2015": "~6.6.0",
    "babel-preset-react": "~6.5.0",
    "babel-preset-stage-3": "~6.5.0",
    "css-loader": "~0.23.1",
    "eslint": "~2.2.0",
    "eslint-config-airbnb": "~6.0.2",
    "eslint-plugin-react": "~4.1.0",
    "mocha": "~2.4.5",
    "nyc": "~5.6.0",
    "postcss-custom-properties": "~5.0.0",
    "postcss-import": "~8.0.2",
    "protractor": "~3.1.1",
    "style-loader": "~0.13.0",
    "stylelint": "~4.4.0",
    "stylelint-config-standard": "~3.0.0",
    "webpack": "~1.12.14",
    "webpack-dev-middleware": "~1.5.1",
    "webpack-hot-middleware": "~2.8.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-3",
      "react"
    ]
  },
  "eslint": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true,
      "mocha": true,
      "jasmine": true
    },
    "rules": {
      "no-use-before-define": [
        2,
        "nofunc"
      ],
      "object-curly-spacing": 0,
      "new-cap": 0
    }
  }
}
